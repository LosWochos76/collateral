<Window x:Class="SeminarManager.EditPersonWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeminarManager"
        mc:Ignorable="d"
        Title="EditPersonWindow" Height="300" Width="400" DialogResult="{Binding Result}">
    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Bottom" Margin="5">
            <Button Margin="2" IsDefault="True">Ok</Button>
            <Button Margin="2" IsCancel="True">Abbrechen</Button>
        </StackPanel>
        <Grid>
            
        </Grid>
        
    </DockPanel>
</Window>
