<Window x:Class="StartEnd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StartEnd"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="400">
    <StackPanel Margin="5">
        <Label>Start:</Label>
        <DatePicker x:Name="start" SelectedDateChanged="DatePicker_SelectedDateChanged" />
        <Label>End:</Label>
        <DatePicker x:Name="end" SelectedDateChanged="DatePicker_SelectedDateChanged" />
        <Button x:Name="button" IsEnabled="False" Click="Button_Click">Ok</Button>
    </StackPanel>
</Window>
